{
  "name": "curriculum-vitae",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "devNoCache": "pwsh -c \"rm -r -fo .next\" && pnpm dev",
    "build": "pnpm exec prisma generate && pnpm exec next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "format": "pnpm prettier --write .",
    "prisma": "pnpm exec prisma db push && pnpm exec prisma generate",
    "post": "pnpm exec tsc ./src/generate.ts && node ./src/generate.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "5.4.2",
    "@types/lodash": "^4.14.200",
    "@types/lru-cache": "^7.10.10",
    "@types/node": "20.8.7",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "autosize": "^6.0.1",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "framer-motion": "^10.16.4",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "next": "13.5.6",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "tailwindcss": "3.3.3",
    "tailwindcss-neumorphism": "^0.1.0",
    "typescript": "5.2.2",
    "zod": "^3.22.4",
    "zod-prisma-types": "^2.8.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.0.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "prisma": "^5.4.2"
  },
  "pnpm": {
    "overrides": {
      "minimist@<0.2.4": ">=0.2.4",
      "minimatch@<3.0.5": ">=3.0.5",
      "minimist@<0.2.1": ">=0.2.1",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "postcss@<7.0.36": ">=7.0.36",
      "postcss@>=7.0.0 <7.0.36": ">=7.0.36",
      "zod@<=3.22.2": ">=3.22.3",
      "postcss@<8.4.31": ">=8.4.31"
    }
  }
}
